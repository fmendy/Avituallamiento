<Window x:Class="Avituallamiento.gui.persona.DialogoFormularioPersona"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DialogoFormularioPersona" Height="300" Width="300">
    <Grid>
        <Button x:Name="jButtonAceptar" Content="Aceptar" HorizontalAlignment="Left" Margin="68,203,0,0" VerticalAlignment="Top" Width="75" Click="jButtonAceptar_Click"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="68,52,0,0" VerticalAlignment="Top" Width="54"/>
        <Label Content="Telefono" HorizontalAlignment="Left" Margin="68,110,0,0" VerticalAlignment="Top" Width="54"/>
        <TextBox x:Name="jTextNombre" Validation.Error="Validation_Error"  Text="{Binding Path=Nombre, NotifyOnValidationError=True, ValidatesOnDataErrors=True}" HorizontalAlignment="Left" Height="23" Margin="136,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66"/>
        <TextBox x:Name="jTextTelefono" Validation.Error="Validation_Error" Text="{Binding Path=Telefono, NotifyOnValidationError=True, ValidatesOnDataErrors=True}" HorizontalAlignment="Left" Height="23" Margin="136,113,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66"/>
        <Button x:Name="jButtonCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="176,203,0,0" VerticalAlignment="Top" Width="75" Click="jButtonCancelar_Click"/>

    </Grid>
</Window>
